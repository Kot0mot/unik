import java.util.Stack;

public class Main {
    public static void main(String[] args) {
        Stack<String> stack = new Stack<>();

        stack.push("a");
        stack.push("b");
        stack.push("c");

        System.out.println("Верхушка стека: " + stack.peek()); // c
        System.out.println("Извлекаем: " + stack.pop());       // c
        System.out.println("Теперь верхушка: " + stack.peek());// b
    }
}
